<Master @type={{@type}} @id={{@id}} @title={{@title}} @subtitle={{@subtitle}} @ts={{@ts}}>
  {{#if @attributes }}
    <table class="card-body">
      <tbody>
        {{! TODO: FIX ATTRIBUTES ITERATION }}
        {{#each-in @attributes as |label value|}}
          <tr>
            <td class="m-1">{{label}}</td>
            <th class="float-end">{{value}}</th>
          </tr>
        {{/each-in}}
      </tbody>
    </table>
  {{/if}}
</Master>

