<div class="container-fluid">
  <div class="row">
    {{#each this.items as |item|}}
      <div class="col-lg-4 col-md-6 col-12">
        {{component item.type id=item.id title=item.meta.title subtitle=item.meta.subtitle ts=item.meta.ts attributes=item.attributes}}
  
        <!--
        <Card
          @type={{item.type}}
          @id={{item.id}}
          @title={{item.meta.title}}
          @subtitle={{item.meta.subtitle}}
          @ts={{item.meta.ts}}
          @attributes={{item.attributes}}
          ></Card>
        -->
      </div>
    {{/each}}
  </div>
</div>
